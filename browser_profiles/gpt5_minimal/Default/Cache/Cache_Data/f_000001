	<script type="text/javascript">
	
	var goLogin, $form, $sendForm;
	var click = true;	
	(function($){

		goLogin = function() {
			var name = $.trim($("#username").val());
			var password = $("#password").val();
			
			var sproKeyModulus = $("#sproKeyModulus").val();
	        var sproKeyExponent = $("#sproKeyExponent").val();

	        var rsa = new RSAKey();
	        rsa.setPublic(sproKeyModulus, sproKeyExponent);

			if(name == "") {
				alert("아이디를 입력하여 주십시오.");
				$("#username").focus();
				return false;
			}
			
			if(password == "") {
				alert("비밀번호를 입력하여 주십시오.");
				$("#password").focus();
				return false;
			}
			
			var domain = document.domain;
			var securedUsername = rsa.encrypt(name);
		    var securedPassword = rsa.encrypt(password);
		    
		    if($("#saveID").is(":checked")) {
				localStorage.setItem("LOGIN_SAVEID", name);
			} else {
				localStorage.removeItem("LOGIN_SAVEID");
			}

		    $("#j_username").val(securedUsername);
		    $("#j_password").val(securedPassword + '');
			
			return true;
		};
		
		$(document).ready(function(){
			
			$form = $("#loginForm");
			$sendForm = $("#loginSendForm");
			var companyName = $.cookie("COMPANY_NAME") || "KT bizmeka EZ";
			if(companyName){
				$("#companyName").text(decodeURIComponent(companyName));
			}
			
			$("#username").focus();
			
			$("#btnSubmit").click(function() {
				$("#bannerUrl").val("");
				var name = $.trim($("#username").val());
				var password = $("#password").val();
				
				if(name == "") {
					alert("아이디를 입력하여 주십시오.");
					$("#username").focus();
					return false;
				}
				
				if(password == "") {
					alert("비밀번호를 입력하여 주십시오.");
					$("#password").focus();
					return false;
				}
				//인증번호로그인 체크 확인
				if($("input[name='loginType']:checked").val() == "captchLogin"){
					var $input = $("input[name=answerData]").val();
					var captcha = $("#answerData").get(0).Captcha;
					if($input.length == 6) {
						var obj_data = new Object();
						obj_data.answerData = $("input[name=answerData]").val();
						obj_data.captchaId = captcha.Id;
						obj_data.instanceId = captcha.InstanceId;
					
						$.ajax({
			 				url : "/loginIsCaptcha.do",
			 				data: obj_data,
			 				type : "post",
			 				success : function(result){
			 					if(result == "FAIL") {
			 						alert("캡차 번호가 맞지 않습니다.\n다시 입력해 주세요.");
			 						$("#answerData").get(0).Captcha.ReloadImage();
			 						return;
			 					}else{
			 						$("input[name=captchaInput]", $sendForm).val($("#answerData").val());
			 						sendFormSubmit();	
			 					}
			 				},
			 				error : function(xhr){
			 					alert("이용에 불편을 드려 죄송합니다.");	
			 				}
			 			});
					} else {
						alert("보안문자 6자리로 입력해 주세요.");
						return false;
					}
				}else{
					sendFormSubmit();
				}
			});
			
			sendFormSubmit = function(){
				if(click){
					click = false;
						
					$sendForm.submit();
				}else{
					 // 타이밍 추가
		            setTimeout(function () {
		                click = true;
		            }, 1000)
					return false;
				}	
			}
			
			$('#password').keypress( function(event) {
				if(event.which == 13) {
					$("#btnSubmit").trigger("click");
					return false;
				}else{
					kc = event.keyCode ? event.keyCode : event.which;
					sk = event.shiftKey ? event.shiftKey : ((kc == 16) ? true : false);
					
					if(((kc >= 65 && kc <= 90) && !sk)||((kc >= 97 && kc <= 122) && sk)){
						$("#capsLockTip").show();
					}else{
						$("#capsLockTip").hide();
					}				
				}
			});
			
			$('#password').focusout(function() {
				$("#capsLockTip").hide();
			});
			
			var savedId = localStorage.getItem("LOGIN_SAVEID");
			if( savedId ){
				$("input[name=username]").val(savedId);
			}
			
			//인증번호 로그인 체크
			var authNumLoginCk = $.cookie("AUTH_NUM_LOGIN");
			if( authNumLoginCk == 1 ){
				$("input[name='loginType']:radio[value='captchLogin']").prop("checked", true); 
				$("#login-container").css("height","500px");
				$(".login-form").css("height","500px");
				$(".login-banner").css("top","500px");
				$("#captchaDiv").css("display","");
				$("#btnSubmit").css("margin","0 0 0 0");	
			}			
			//인증번호 로그인 체크 값 변경시
			$("input[name='loginType']").change(function(event) {
				if($("input[name='loginType']:checked").val() == "authNumLogin"){
					$("#login-container").css("height","360px");
					$(".login-form").css("height","360px");
					$(".login-banner").css("top","360px");
					$("#captchaDiv").css("display","none");
					$("#btnSubmit").css("margin","30px 0 4px");
					$.cookie("AUTH_NUM_LOGIN", "0", {path:"/", expires:new Date(9999, 12, 31, 0, 0, 0)});
				}else{
					$.cookie("AUTH_NUM_LOGIN", "1", {path:"/", expires:new Date(9999, 12, 31, 0, 0, 0)});
					$("#login-container").css("height","500px");
					$(".login-form").css("height","500px");
					$(".login-banner").css("top","500px");
					$("#captchaDiv").css("display","");
					$("#btnSubmit").css("margin","0 0 0 0");
				}
			});
			//---------------------------------------------- 보안문자(캡차) 로직 시작 -------------------------------------------- //
			$("#captchaBtnReload").click(function(event) {
				event.preventDefault();
				$("#answerData").get(0).Captcha.ReloadImage();
			});
			$("#captchaBtnReload2").click(function(event) {
				event.preventDefault();
				$("#answerData").get(0).Captcha.ReloadImage();
			});
			$("#captchaBtnSound").click(function(event) {
				event.preventDefault();
				$("#answerData").get(0).Captcha.PlaySound();
			});
			
			$("#answerData").keypress( function(event) {
				if(event.which == 13) {
					$("#btnSubmit").trigger("click");
					return false;
				}
			});
			
			//---------------------------------------------- 보안문자(캡차) 로직 종료  -------------------------------------------- //
			$('.side-bar').click(function(){
				var $leftmenu = $('#sidemenu');
				
				if($leftmenu.hasClass('opened')){
					$leftmenu.removeClass('opened');
				}else{
					$leftmenu.addClass('opened');
				}
			});
			
			$("#loginGuide").click(function(event) {
	 			event.preventDefault();
	 			
	 			$("#divLoginGuide").modal("show");
	 		});
			
			var version = Number($.browser.version);
	 		if($.browser && ($.browser.msie && version < 9)) {
	 			var $noti = $('<div class="ie8-niti">' +
						'<h1>고객님께서 사용하시는 브라우저는 Internet Explorer 8 이하 버전입니다.</h1>' +
					  	'<p>KT bizmeka EZ는 Internet Explorer 9 이상에 최적화되어 있습니다.<br>현재 사용하시는 Internet Explorer를 업그레이드하거나, 다른 브라우저(크롬, 파이어폭스)를<br>사용하시기 바랍니다.<br/><a href="http://windows.microsoft.com/ko-KR/internet-explorer/download-ie" target="_blank">Internet Explorer 최신버전 다운로드</a>&nbsp;&nbsp;<a href="https://www.google.com/chrome/browser/desktop/index.html" target="_blank">크롬 다운로드</a>&nbsp;&nbsp;<a href="https://www.mozilla.org/ko/firefox/new/" target="_blank">파이어폭스 다운로드</a></p>' +
					  	'<a href="#a" class="btn-close"><span>X</span><span class="blind">닫기</span></a>' +
					'</div>');
						
				$("a.btn-close", $noti).click(function() {
					$noti.remove();
				});

				$noti.appendTo(document.body);
	 		}

	 		$("a[data-role='banner']").on("click", function(){
	 			var itemId = $(this).data("bannerId");
	 			var itemUrl = $(this).data("bannerUrl");
	 			var itemOption = $(this).data("bannerOption");
	 			var windowName = $(this).data("bannerName");
	 			$.ajax({
	 				url : "/support/accesslog/createAccessLog.do",
	 				data: {
	 					"systemId" : "SO",
	 					"itemId" : itemId
	 				},
	 				type : "post",
	 				success : function(data){},
	 				error : function(xhr){}
	 			});
				if(itemOption == ""){
					var openNewWindow = window.open("about:blank");
					openNewWindow.location.href = itemUrl;	
				}else{
		 			spro.popupOpen(itemUrl,itemOption,windowName);
				}
			});	 		
		});
		
		ktCompanySuspension = function(){
			event.preventDefault();
			
			$("#divKtCompanySuspension").modal({backdrop: 'static'});	
		} 
		ktCompanySuspensionCloese = function(){
			jQuery('#divKtCompanySuspension').modal('hide')
			location.href="/loginForm.do";
		} 
		
		logoImageError = function(img) {
			$(img).replaceWith('<span class="logo">KT bizmeka EZ</span>');
		};
		
		
		goBanner = function(url, msg){
			$("#bannerUrl").val(url);
			var name = $.trim($("#username").val());
			var password = $("#password").val();
			var htmlMsg = "";
			if(name == "") {
				htmlMsg = "<span>로그인 후 이동할 수 있습니다.<br/>아이디를 입력 후 <span style='color: #e31010; text-decoration: underline;'>"+msg+"</span></span>";				
				loginAlertMsg(htmlMsg);
				$("#username").focus();
				return false;
			}
			
			if(password == "") {
				htmlMsg = "<span>로그인 후 이동할 수 있습니다.<br/>비밀번호를 입력 후 <span style='color: #e31010; text-decoration: underline;'>"+msg+"</span></span>";
				loginAlertMsg(htmlMsg);
				
				$("#password").focus();
				return false;
			}
			//인증번호로그인 체크 확인
			if($("input[name='loginType']:checked").val() == "captchLogin"){
				var $input = $("input[name=answerData]").val();
				var captcha = $("#answerData").get(0).Captcha;
				if($input.length == 6) {
					var obj_data = new Object();
					obj_data.answerData = $("input[name=answerData]").val();
					obj_data.captchaId = captcha.Id;
					obj_data.instanceId = captcha.InstanceId;
				
					$.ajax({
		 				url : "/loginIsCaptcha.do",
		 				data: obj_data,
		 				type : "post",
		 				success : function(result){
		 					if(result == "FAIL") {
		 						alert("캡차 번호가 맞지 않습니다.\n다시 입력해 주세요.");
		 						$("#answerData").get(0).Captcha.ReloadImage();
		 						return;
		 					}else{
		 						$("input[name=captchaInput]", $sendForm).val($("#answerData").val());
		 						sendFormSubmit();	
		 					}
		 				},
		 				error : function(xhr){
		 					alert("이용에 불편을 드려 죄송합니다.");	
		 				}
		 			});
				} else {
					htmlMsg = "<span>로그인 후 이동할 수 있습니다.<br/>보안문자 6자리를 입력 후  <span style='color: #e31010; text-decoration: underline;'>"+msg+"</span></span>";
					loginAlertMsg(htmlMsg);
					//alert("로그인 후 이동할 수 있습니다.\n보안문자 6자리를 입력 후 지인 추천 배너를 클릭해 주세요.");
					$("#answerData").focus();
					return false;
				}
			}else{
				sendFormSubmit();
			}
		}
		
		loginAlertMsg = function(htmlMsg){
			$("#modalMsg").empty();
			$("#modalMsg").append(htmlMsg);			
			$("#divLoginAlertMsg").modal("show");
		} 
		
		

	})(jQuery);
	
	function openLoginSettingGuide() {
		spro.popupOpen('https://ezportal.bizmeka.com/html/userTipNew/index.html?page=html/po-docs/po_22.html',{width:1300, height:720});
	}
	

	function newWindowOpen(url){
		window.open(url, "_blank");
	}
	</script>
