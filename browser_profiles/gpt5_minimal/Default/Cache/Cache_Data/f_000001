	<script type="text/javascript">
		var PO_BASE_URL = "https://ezportal.bizmeka.com";

		if(opener) {	
			var ctl = true;	
			var regex = new RegExp("^([0-9A-Za-z-\\.@:%_\+~#=]+)+((\\.[a-zA-Z]{2,3})+)(/(.)*)?(\\?(.)*)?");
			var ts = location.href;
			
			try {
				opener.document;
			} catch(e) {
				document.domain = location.hostname.replace(/([a-zA-Z0-9-]+.)/, "");
				try {
					opener.document;
				} catch(e) { ctl = false; }
			}
			
			if(ctl) {
				if (regex.test(ts)) {
					opener.location.href = ts;
					window.close();
				} else {
// 					alert("올바른 요청이 아닙니다.");
				}				
				
			}
		} else if(top != window) {	
		
			var regex = new RegExp("^([0-9A-Za-z-\\.@:%_\+~#=]+)+((\\.[a-zA-Z]{2,3})+)(/(.)*)?(\\?(.)*)?");
			var ts = location.href;
			
			try {
				top.document;
			} catch(e) {
				document.domain = location.hostname.replace(/([a-zA-Z0-9]+.)/, "");
			}
			if (regex.test(ts)) {			
				top.location.href = ts;
			} else {
				top.location.reload();
// 				alert("올바른 요청이 아닙니다.");
			}
		}
		
	</script>
